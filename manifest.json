{ 
    "name": "Productivify",
    "version": "1.0.0",
    "description": "Limit distractions and focus on productivity.",
    "manifest_version": 2,
    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },
    "permissions": [ "tabs", "storage" ],
    "browser_action": { "default_popup": "popup.html"}
}